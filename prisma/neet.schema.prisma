generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/@prisma/client/neet"
}

datasource db {
  provider = "mysql"
  url      = env("NEET_DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  password String
  name  String?
}